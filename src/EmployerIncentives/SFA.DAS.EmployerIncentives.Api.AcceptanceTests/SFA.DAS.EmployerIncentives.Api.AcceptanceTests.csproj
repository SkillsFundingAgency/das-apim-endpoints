    <Compile Update="Features\EmploymentCheck.feature.cs">
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Features\UpdateCollectionCalendarPeriod.feature.cs">
      <DesignTime>True</DesignTime>
