    </Compile>
    <Compile Update="Features\PausePayments.feature.cs">
      <DependentUpon>PausePayments.feature</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Data\TestData.resx">
